<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="10dp" android:top="10dp">
        <shape
            android:shape="rectangle">

            <corners android:bottomLeftRadius="35dp"
                android:topLeftRadius="35dp"
                android:bottomRightRadius="35dp"
                android:topRightRadius="35dp"/>
            <gradient android:startColor="@color/colorGradientStart1_2"
                android:centerColor="@color/colorMiddleStart1_2"
                android:endColor="@color/colorGradientEnd1_2"
                android:angle="-45"/>

        </shape>
    </item>
    <item android:bottom="7dp" android:right="7dp">
        <shape
            android:shape="rectangle">

            <corners android:bottomLeftRadius="35dp"
                android:topLeftRadius="35dp"
                android:bottomRightRadius="35dp"
                android:topRightRadius="35dp"/>
            <gradient android:startColor="@color/colorGradientStart"
                android:centerColor="@color/colorMiddleStart"
                android:endColor="@color/colorGradientEnd"
                android:angle="-45"/>

        </shape>
    </item>
</layer-list>