<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="10dp" android:top="10dp">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="35dp"
                android:topLeftRadius="35dp"
                android:bottomRightRadius="35dp"
                android:topRightRadius="35dp"
                />
            <gradient android:startColor="@color/colorGradientStart2_2"
                android:endColor="@color/colorGradientEnd2_2"
                android:angle="135"/>
        </shape>
    </item>
    <item android:bottom="6dp" android:right="4dp">
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/colorGradientStart2"
                android:endColor="@color/colorGradientEnd2"
                android:angle="135"/>
            <corners android:bottomLeftRadius="35dp"
                android:topLeftRadius="35dp"
                android:bottomRightRadius="35dp"
                android:topRightRadius="40dp"
                />


        </shape>
    </item>
</layer-list>